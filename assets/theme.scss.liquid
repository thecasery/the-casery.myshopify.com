  vertical-align:middle;
  padding-left: 50px;
  display:inline-block;
  width: 56%;
}

.product-container-right{
  vertical-align:middle;
  padding-left: 50px;
  display:inline-block;
  width: 43%;
}

.product-img-media__container{
  width: 84%;
  display: inline-block;
  vertical-align: top;
  text-align: center;
}

.product-img{
