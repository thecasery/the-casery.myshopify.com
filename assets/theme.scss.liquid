}

.active-option1{
  border: 1px solid orange;
}

.active-option3{
  border: 1px solid orange;
}

.slider-dots{
  display: none;
}

.product-pc-money{
  display: block;
  margin-top:-10px;
}

.product-mobile-money{
  display: none;
}

.case-description{
  display: none;
}

.active-case-description{
  display: block;
}

#AddToCartForm{
