/* Magnifier here*/
.magnifier-cover{
  position: absolute; 
  cursor: -moz-zoom-in; 
  cursor: -webkit-zoom-in; 
  cursor: zoom-in; 
  background-color: #f0f0f0;  
  border: 1px solid #d0d0d0;
  opacity: 0.4;
  display: none;
}

.magnifier-content{
  position:absolute; 
  overflow:hidden; 
  display:none;
}

.product-img__container .active-magnifier{
  opacity: 0.8;
  cursor: -moz-zoom-out; 
  cursor: -webkit-zoom-out; 
  cursor: zoom-out; 
}

