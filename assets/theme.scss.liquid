  margin: 0;
  cursor:pointer;
}

.option2{
  margin-top: 12px;
  padding-bottom: 5px;
}

.option3{
  width: 15%;
  display: inline-block;
  text-align: left;
  vertical-align: top;
  padding-right: 10px;
}

.option2-outer-container img{
  display:inline-block; 
  margin: 0;
  cursor:pointer;
}

.option2 .option2-outer-container .active-option2{
